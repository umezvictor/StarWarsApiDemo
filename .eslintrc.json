{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "off",
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit":"off",
        "object-shorthand":"off",
        "class-methods-use-this":"off",
        "no-shadow":"off",
        "no-plusplus":"off",
        "global-require":"off",
        "consistent-return": "off",
        "strict":"off"
    }
}